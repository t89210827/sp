      <view class='center-style' style='line-height:200rpx;'>审核员工</view>
    </navigator>
  </view>
</view>

<view style='width:750rpx;'>
  <view style='width:90%;height:200rpx;margin:40rpx 5%;box-shadow: 0px 0px 10px #E3F6FD;'>
    <navigator url='/pages/shopManager/boutiqueDetail/boutiqueDetail'>
      <view class='center-style' style='line-height:200rpx;'>提交竞品日报</view>
    </navigator>
  </view>
</view>

<view style='width:750rpx;'>
  <view style='width:90%;height:200rpx;margin:40rpx 5%;box-shadow: 0px 0px 10px #E3F6FD;'>
    <navigator url='' bindtap='judgeProduct'>
      <view class='center-style' style='line-height:200rpx;'>提交产品日报</view>
    </navigator>
  </view>
</view>

